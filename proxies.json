{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "demo-api": {
            "matchCondition": {
                "route": "/apidemo/{id}",
                "methods": [
                    "GET",
                    "DELETE"
                ]
            },
            "backendUri": "https://!!changeme!!.azurewebsites.net/api/exampleApi_{request.method}?id={id}&code=!!changeme!!"
        }
    }
}
